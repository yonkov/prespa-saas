					"name": "Gray 50"
				},
				{
					"slug": "dark-gray",
					"color": "#4e4e4e",
					"name": "Dark Gray"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "silver-blue",
					"color": "#ebeefc",
					"name": "Silver Blue"
				},
				{
					"slug": "blue",
					"color": "#356df1",
					"name": "Blue"
				},
				{
					"slug": "links",
					"color": "#2253cb",
					"name": "Links"
